{
  "name": "atelier",
  "jest": {
    "testEnvironment": "jest-environment-jsdom-global",
    "setupFilesAfterEnv": [
      "./setup-tests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "version": "1.0.0",
  "description": "FEC Project Hack Reactor",
  "main": "webpack.config.js",
  "scripts": {
    "test": "jest --coverage",
    "client-dev": "webpack --mode development --watch",
    "server-dev": "npx nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Octothorpes/Atelier.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Octothorpes/Atelier/issues"
  },
  "homepage": "https://github.com/Octothorpes/Atelier#readme",
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "axios": "^0.21.1",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "css-loader": "^6.2.0",
    "dayjs": "^1.10.7",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "jest": "^27.1.0",
    "jquery": "^3.6.0",
    "mocha": "^9.1.1",
    "react": "^17.0.2",
    "react-async": "^10.0.1",
    "react-dom": "^17.0.2",
    "style-loader": "^3.2.1",
    "underscore": "^1.13.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  },
  "devDependencies": {
    "babel-jest": "^27.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest-environment-jsdom-global": "^3.0.0",
    "react-test-renderer": "^17.0.2"
  }
}
